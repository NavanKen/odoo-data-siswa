<odoo>
    <record id="view_kota_graph" model="ir.ui.view">
        <field name="name">kota.graph</field>
        <field name="model">siswa_property</field>
        <field name="arch" type="xml">
            <graph string="Jumlah Siswa Berdasarkan Kota" type="bar">
                <field name="kota_id"/>
                <field name="id" name="count"/>
            </graph>
        </field>
    </record>
</odoo>
