<odoo>
    <record id="siswa_property_view_tree" model="ir.ui.view">
        <field name="name">siswa.property.tree</field>
        <field name="model">siswa_property</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="kota_id"/>
                <field name="jurusan_id"></field>
                <field name="nis"/>
                <field name="tanggal_lahir"/>
                <field name="age"/>
                <field name="asal_sekolah"/>
                <field name="gender"/>
            </tree>
        </field>
    </record>

    <record id="siswa_property_view_form" model="ir.ui.view">
        <field name="name">siswa.property.form</field>
        <field name="model">siswa_property</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="kota_id"/>
                        <field name="jurusan_id"></field>
                        <field name="nis"/>
                        <field name="tanggal_lahir"/>
                        <field name="age"/>
                        <field name="asal_sekolah"/>
                        <field name="gender"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
